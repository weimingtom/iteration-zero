/**
 GLUI_Bitmap is a simple 2D texture map.  It's used
 to represent small textures like checkboxes, arrows, etc.
 via the GLUI_StdBitmaps class.
*/
struct Bitmap 
{
    public:

    /** Create bitmap from grayscale byte image */
    void init_grayscale(ubyte* array)
    {
        format = GL_LUMINANCE;
        m_width = array[0]; m_height = array[1];
        pixels = array[2..m_width*m_height];
    }
    
    void draw(Point p)
    {
        if (width*height != 0 ) 
        {
            glPixelStorei(GL_UNPACK_ALIGNMENT,1);
            glRasterPos2f(0.5f + p.x, 0.5f + p.y + height);
            glDrawPixels( 
                width, height, format, GL_UNSIGNED_BYTE, pixels.ptr);
        }
    }
    int width() { return m_width; }
    int height() { return m_height; }
    Size size() { return Size(m_width,m_height); }

private:
    ubyte[] pixels;
    int m_width=0, m_height=0;
    uint format = GL_LUMINANCE;
}


/**
 * Keeps an array of Bitmap objects to represent all the 
 * images used in the UI: checkboxes, arrows, etc.
 */
class StdBitmapSet
{
    public:
    this()
    {
        for (int i=0; i<bitmaps.length; i++) {
            bitmaps[i].init_grayscale(bitmap_arrays[i]);
        }
    }
    ~this() {};

    /** Return the width (in pixels) of the n'th standard bitmap. */
    int  width (int i)
    {
        assert(i>=0 && i<=StdBitmaps.max);
        return bitmaps[i].width;
    }
    /** Return the height (in pixels) of the n'th standard bitmap. */
    int  height(int i)
    {
        assert(i>=0 && i<=StdBitmaps.max);
        return bitmaps[i].height;
    }

    /** Draw the n'th standard bitmap (one of the enums
       listed in GLUI_StdBitmaps_Codes) at pixel corner (x,y). 
    */
    void draw(int i, Point p)
    {
        assert(i>=0 && i<=StdBitmaps.max);
        bitmaps[i].draw(p);
    }

    Bitmap opIndex(uint i) { return bitmaps[i]; }

    private:
    Bitmap[StdBitmaps.max+1] bitmaps;
};




/**
